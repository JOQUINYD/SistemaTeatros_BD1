@model IEnumerable<SistemaTeatroWebApp.Models.Usuarios>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accesos.Tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Personas.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accesos.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Personas.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Usuario }) |
            @Html.ActionLink("Details", "Details", new { id=item.Usuario }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Usuario })
        </td>
    </tr>
}

</table>
